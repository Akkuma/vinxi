{
  "name": "@vinxi/plugin-mdx",
  "version": "3.7.1",
  "type": "module",
  "scripts": {
    "// User Scripts": "",
    "dev": "pnpm run tsc:watch",
    "types": "pnpm run tsc:once",
    "// TypeScript": "",
    "tsc:watch": "tsc --build --watch",
    "tsc:once": "tsc --build",
    "build:cjs": "esbuild --bundle --platform=node --external:esbuild --external:vite --external:@mdx-js/mdx src/index.ts > dist/index.cjs",
    "// Release": "",
    "prepublishOnly": "pnpm run build:cjs",
    "release": "npm version patch && npm publish && git push && git push --tags",
    "// Remove caches": "",
    "clean": "git clean -Xdff"
  },
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@mdx-js/mdx": "<3"
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "esbuild": "0.18.7",
    "resolve": "^1.22.4",
    "unified": "^9.2.2",
    "vfile": "^5.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "2.3.0",
    "@types/es-module-lexer": "^0.3.0",
    "@types/mdast": "^3.0.12",
    "@types/node": "^14.18.56",
    "@types/resolve": "^1.20.2",
    "@types/unist": "^2.0.7",
    "typescript": "^5.2.2",
    "vite": "4.5.0"
  },
  "description": "Vinxi plugin for MDX",
  "license": "MIT",
  "repository": "https://github.com/nksaraf/vinxi/blob/main/packages/vinxi-mdx",
  "keywords": [
    "vinxi",
    "mdx"
  ],
  "publishConfig": {
    "access": "public"
  }
}
